---
import DocsSection from "@/components/docs-section.astro";
import V1FunctionCard from "@/components/v1/function-card.astro";
import DocsPage from "@/layouts/docs-page.astro";
import { CATEGORIZED } from "@/lib/v1/categorized";
---

<DocsPage showVersionSelector title="v1 (Legacy)" categorized={CATEGORIZED}>
  {
    CATEGORIZED.map(([category, functions]) => (
      <DocsSection title={category}>
        {functions.map((func) => (
          <V1FunctionCard func={func} />
        ))}
      </DocsSection>
    ))
  }
</DocsPage>
