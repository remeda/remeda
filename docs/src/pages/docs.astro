---
import DocsPage from "@/layouts/docs-page.astro";
import FunctionCard from "@/components/function-card.astro";
import { CATEGORIZED } from "@/lib/categorized";
import { flat, map, pipe, values } from "remeda";
---

<DocsPage showVersionSelector title="Documentation" categorized={CATEGORIZED}>
  {
    pipe(
      CATEGORIZED,
      values(),
      flat(),
      map((func) => <FunctionCard func={func} />),
    )
  }
</DocsPage>
