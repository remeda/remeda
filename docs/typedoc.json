{
  "$schema": "https://typedoc.org/schema.json",

  "tsconfig": "../tsconfig.json",
  "entryPoints": ["../src/index.ts"],
  "exclude": ["**/*.test.ts"],

  "blockTags": [
    "@category",
    "@dataFirst",
    "@dataLast",
    "@example",
    "@exampleRaw",
    "@indexed",
    "@param",
    "@pipeable",
    "@signature",
    "@strict"
  ],

  "jsDocCompatibility": {
    "exampleTag": false
  },

  "excludeNotDocumented": true
}
