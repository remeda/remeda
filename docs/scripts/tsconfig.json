{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
